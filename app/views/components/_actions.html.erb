<%- if can? :edit, obj %>
    <%= link_to "Edit #{obj.model_name}", edit_polymorphic_path(obj), class: 'action' %>
    <% if obj.is_a? Book %>
    <%= link_to "Add Praise", new_book_praise_path(obj), class: 'action' %>
    <%= link_to "Writing Goal", writing_goal_path(obj.writing_goal), class: 'action' %>
    <%end%>
<%- end %>
